<h1> Posts </h1>
<%= link_to 'new post',new_post_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="3"></th>
  </tr>

  <% @posts.each do |post| %>
   <tr>
      <td><%= post.title %></td>
      <td><%= post.body %></td>
      <td><%= image_tag post.image.url(:medium) %></td>
      <td><%= link_to 'Show', post_path(post) %></td>
    </tr>
<% end %>
</table>
