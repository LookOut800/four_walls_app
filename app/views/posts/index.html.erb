<h1> Posts </h1>
<%= link_to 'new post',new_post_path %>
<table>
  <% @posts.each do |post| %>
   <tr>
      <td>
        <%= post.title %><br>
        <%= post.body %><br>
        <%= image_tag post.image.url(:medium) %><br>
        <%= link_to 'Show', post_path(post) %>
      </td>
    </tr>
<% end %>
</table>
